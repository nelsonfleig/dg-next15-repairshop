{
  "extends": ["next/core-web-vitals", "next/typescript", "prettier"],
  "plugins": ["unused-imports"],
  "rules": {
    "prefer-const": "error",
    "unused-imports/no-unused-imports": "warn",
    "@typescript-eslint/no-unused-vars": [
      "warn",
      {
        "argsIgnorePattern": "^_",
        "varsIgnorePattern": "^_",
        "caughtErrorsIgnorePattern": "^_"
      }
    ]
  }
}
